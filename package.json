{
  "name": "@yohames/test_bumpp",
  "version": "1.2.0",
  "description": "",
  "author": "ss-libs",
  "license": "Closed",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yohames/testing_bumpp.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "keywords": [],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "README.md",
    "dist",
    "package.json"
  ],
  "scripts": {
    "build": "tsdown",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "typecheck": "tsc --noEmit",
    "prepare": "npx simple-git-hooks",
    "prepublishOnly": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "release": "bumpp --commit --push --tag",
    "tf:pipe": "tsx scripts/pipeline-submit-tf.ts"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.4.1",
    "@types/node": "^24.7.2",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "bumpp": "^10.3.1",
    "eslint": "^9.37.0",
    "eslint-plugin-format": "^1.0.2",
    "lint-staged": "^16.2.4",
    "simple-git-hooks": "^2.13.1",
    "tsdown": "^0.15.7",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
